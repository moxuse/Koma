{
  "name": "msplr",
  "version": "0.9.0",
  "description": "",
  "main": "main.js",
  "scripts": {
    "dev": "run-p dev:*",
    "dev:electron": "wait-on ./dist/main/index.js && cross-env NODE_ENV=\"development\" electron dist/main",
    "dev:tsc": "tsc -w -p tsconfig.main.json",
    "dev:webpack": "webpack --watch",
    "test": "jest",
    "app:dir": "electron-builder --dir",
    "build:osx": "rimraf dist && webpack --mode=production && electron-builder --mac --x64"
  },
  "author": "moxus",
  "license": "MIT",
  "build": {
    "asar": true,
    "extends": null,
    "extraMetadata": {
      "main": "dist/main/index.js"
    },
    "files": [
      "dist/index.html",
      "dist/app.js",
      "dist/synthDef/*",
      "dist/sound/*",
      "dist/font/*"
    ],
    "appId": "org.moxus.msplr",
    "mac": {
      "target": "dmg"
    }
  },
  "dependencies": {
    "assert": "^2.0.0",
    "constants": "^0.0.2",
    "dgram": "^1.0.1",
    "electron-store": "^8.0.1",
    "fs-extra": "^10.0.0",
    "graceful-fs": "^4.2.8",
    "immutable": "^4.0.0",
    "md5": "^2.3.0",
    "osc-js": "^2.2.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.6",
    "redux": "^4.1.2",
    "redux-devtools-extension": "^2.13.9",
    "redux-logger": "^3.0.6",
    "redux-persist": "^6.0.0",
    "redux-persist-transform-immutable": "^5.0.0",
    "redux-thunk": "^2.4.1",
    "styled-components": "^5.3.3",
    "supercolliderjs": "^1.0.1",
    "util": "^0.12.4",
    "wav-decoder": "^1.3.0",
    "ws": "^8.4.0"
  },
  "devDependencies": {
    "@swc/core": "^1.2.112",
    "@swc/helpers": "^0.2.14",
    "@types/copy-webpack-plugin": "^8.0.1",
    "@types/electron-load-devtool": "^1.2.1",
    "@types/jest": "^27.0.3",
    "@types/md5": "^2.3.1",
    "@types/mini-css-extract-plugin": "^2.4.0",
    "@types/node": "^16.11.10",
    "@types/react": "^17.0.36",
    "@types/react-dom": "^17.0.11",
    "@types/redux-logger": "^3.0.9",
    "@types/redux-persist": "^4.3.1",
    "@types/styled-components": "^5.1.15",
    "copy-webpack-plugin": "^10.0.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.5.1",
    "electron": "^16.0.5",
    "electron-builder": "^22.14.5",
    "electron-load-devtool": "^1.2.0",
    "electron-reload": "^2.0.0-alpha.1",
    "html-webpack-plugin": "^5.5.0",
    "jest": "^27.4.3",
    "mini-css-extract-plugin": "^2.4.5",
    "npm-run-all": "^4.1.5",
    "process": "^0.11.10",
    "rimraf": "^2.6.3",
    "style-loader": "^3.3.1",
    "swc-loader": "^0.1.15",
    "ts-jest": "^27.1.0",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.2",
    "wait-on": "^6.0.0",
    "webpack": "^5.64.2",
    "webpack-cli": "^4.9.1"
  }
}
